{
  "name": "@ajhenry/goto",
  "description": "ðŸª„ A cli tool to magically goto folders and github repos locally",
  "version": "0.1.0",
  "author": "Andrew Henry <ajhenry@ajhenry.dev>",
  "bin": {
    "goto-cli": "./bin/run"
  },
  "bugs": "https://github.com/ajhenry/goto/issues",
  "dependencies": {
    "@expo/spawn-async": "^1.6.0",
    "@oclif/core": "^1.9.6",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/fs-extra": "^9.0.13",
    "ansi-escapes": "^5.0.0",
    "conf": "^10.1.2",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2",
    "tslog": "^3.3.3",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.9.4",
    "@types/prompts": "^2.0.14",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ajhenry/goto",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "goto",
    "dirname": "goto",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "default": ".",
    "additionalHelpFlags": ["-h", "--help"]
  },
  "repository": "ajhenry/goto",
  "scripts": {
    "dev": "tsc -w",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
